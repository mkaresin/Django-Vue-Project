ed = true;\n        this.loading = false;\n        this.page = Number(this.page)\n        this.articles = response.data.results;\n      }, (error)  =>  {\n        this.pulled = true;\n        this.loading = false;\n      })\n    },\n    setpulled: function(){\n      this.pulled = true;\n    }\n    \n  \n  },\n}\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<!-- <style scoped>\nh3 {\n  margin: 40px 0 0;\n}\nul {\n  list-style-type: none;\n  padding: 0;\n}\nli {\n  display: inline-block;\n  margin: 0 10px;\n}\na {\n  color: #42b983;\n}\n\n.articles{\n\n  padding: 30px;\n  width: 60%;\n  display: inline-block;\n  margin: 0 10px;\n}\n.hide{\n  display:none;  \n}\n\n.imagecontent{\n  display: inline-block;\n  margin: 0 auto;\n  width: auto;\n\n}\n.pubdate{\n  float: right;\n}\n\n\n</style> -->\n\n<style scoped>\n\n  /*.holder {\n    background: #fff;\n  }\n\n  ul {\n    margin: 0;\n    padding: 0;\n    list-style-type: none;\n  }\n  \n  ul li {\n    padding: 20px;\n    font-size: 1.3em;\n    background-color: #E0EDF4;\n    border-left: 5px solid #3EB3F6;\n    margin-bottom: 2px;\n    color: #3E5252;\n  }\n\n  p {\n    text-align:center;\n    padding: 30px 0;\n    color: gray;\n  }\n\n  .container {\n    box-shadow: 0px 0px 40px lightgray;\n  }*/\n@import'~bootstrap/dist/css/bootstrap.css'\n</style>"]}]}